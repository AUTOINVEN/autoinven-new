                  </svg>
                  <p class="text-base font-bold">info@autoingroup.com</p>
                </div>
              </div>
              <!--창고 상세정보 타이틀-->
    
            <div class="swiper swiper mt-10">
              <!-- Slides -->
              <div class="swiper-wrapper">
                <% if(warehouse.images.length > 0) { %> <% for(let i in
                warehouse.images) { %>
                <div class="swiper-slide">
                  <img id="warehouse_image_<%= i%>" class="slider-image" onerror="this.src='/image/default-image.png'" />
                  <script>
                    document
                      .getElementById('warehouse_image_<%= i%>')
                      .setAttribute('src', '<%=warehouse.images[i]%>');
                  </script>
                </div>
                <% } %> <% } else { %> <% for(let i=0 ; i < 3; i++) { %>
                <div class="swiper-slide">
                  <img id="warehouse_image_<%= i%>" class="slider-image" />
                  <script>
                    document
                      .getElementById('warehouse_image_<%= i%>')
                      .setAttribute('src', '/image/default-image.png');
                  </script>
                </div>
                <% } %> <% } %>
